{
  "name": "ng-doc",
  "version": "0.0.0",
  "license": "MIT",
  "description": "The documentation engine for your Angular projects.",
  "watch": {
    "serve": "libs/**/*.*"
  },
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js",
    "serve": "nx serve",
    "poc": "nx run ng-doc:serve:poc",
    "start": "npm-watch",
    "build": "nx build",
    "lint": "npx nx run-many --all --target=lint",
    "test": "npx nx run-many --all --target=test",
    "build:analyze": "nx build --stats-json --named-chunks",
    "prepare": "node ./prepare.js",
    "debug": "nx g @angular/ssr:ng-add",
    "dev:ssr": "ng run ng-doc:serve-ssr",
    "serve:ssr": "node dist/ng-doc/server/main.js",
    "build:ssr": "ng build && ng run ng-doc:server",
    "prerender": "ng run ng-doc:prerender",
    "migr": "ng g ./dist/libs/builder:ng-update"
  },
  "keywords": [
    "angular",
    "documentation",
    "generator",
    "builder",
    "guides",
    "api",
    "demo",
    "ng-doc"
  ],
  "author": "Skoropad Aleksandr <alex@skoropad.dev>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ng-doc/ng-doc.git"
  },
  "bugs": {
