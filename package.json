{
	"name": "ng-doc",
	"version": "0.0.0",
	"license": "MIT",
	"description": "The documentation engine for your Angular projects.",
	"watch": {
		"serve": "libs/**/*.*"
	},
	"scripts": {
		"ng": "nx",
		"postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
		"serve": "nx serve",
		"start": "npm-watch",
		"build": "nx build",
		"test": "nx test",
		"build:analyze": "nx build --stats-json && webpack-bundle-analyzer dist/apps/ng-doc/stats.json",
		"prepare": "node ./prepare.js"
	},
	"keywords": [
		"angular",
		"documentation",
		"generator",
		"builder",
		"guides",
		"api",
		"demo",
		"ng-doc"
	],
	"author": "Skoropad Aleksandr <alex@skoropad.dev>",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/skoropadas/ng-doc.git"
	},
	"bugs": {
