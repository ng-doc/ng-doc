{
  "singleQuote": true,
  "trailingComma": "all",
  "tabWidth": 4,
  "endOfLine": "crlf",
  "printWidth": 120,
  "semi": true,
  "bracketSpacing": false,
  "jsxBracketSameLine": true,
  "overrides": [
	  {
		  "files": [
			  "*.ts"
		  ],
		  "options": {
			  "parser": "typescript"
		  }
	  },
	  {
		  "files": [
			  "*.ts.nunj"
		  ],
		  "options": {
			  "parser": "typescript"
		  }
	  },
	  {
		  "files": [
			  "*.md.nunj"
		  ],
		  "options": {
			  "parser": "markdown"
		  }
	  }
  ]
}
