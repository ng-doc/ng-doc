{
	"singleQuote": true,
	"trailingComma": "all",
	"tabWidth": 2,
	"endOfLine": "lf",
	"printWidth": 120,
	"semi": true,
	"bracketSpacing": false,
	"bracketSameLine": true,
	"overrides": [
		{
			"files": [
				"*.ts"
			],
			"options": {
				"parser": "typescript"
			}
		},
		{
			"files": [
				"*.ts.nunj"
			],
			"options": {
				"parser": "typescript"
			}
		}
	]
}
