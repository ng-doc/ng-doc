{
	"editorconfig": true,
	"singleQuote": true,
	"trailingComma": "all",
	"semi": true,
	"bracketSameLine": true,
	"overrides": [
		{
			"files": [
				"*.ts"
			],
			"options": {
				"parser": "typescript"
			}
		},
		{
			"files": [
				"*.ts.nunj"
			],
			"options": {
				"parser": "typescript"
			}
		}
	]
}
