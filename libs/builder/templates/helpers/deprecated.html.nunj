{% macro render(member) %}
    {% set notes = member | getJsDocTag('deprecated') %}

    {% if notes %}
        <ng-doc-blockquote type="warning">
            <b>Deprecated</b>
            {{ notes }}
        </ng-doc-blockquote>
    {% endif %}
{% endmacro %}

{% macro class(member) -%}
    {{ "ng-doc-deprecated" if member | hasJsDocTag('deprecated') }}
{%- endmacro %}
