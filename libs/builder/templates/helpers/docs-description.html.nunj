{% import "helpers/tag.html.nunj" as tagMacro %}
{% import "helpers/no-content.html.nunj" as noContentMacro %}

{% macro render(docs) %}
    {% if docs.length %}
        {% for doc in docs %}
        <p>{{ doc.description }}</p>
        {% endfor %}
    {% else %}
        {{ noContentMacro.render('No documentation has been provided.') }}
    {% endif %}
{% endmacro %}
