{
	"$schema": "./node_modules/nx/schemas/nx-schema.json",
	"npmScope": "ng-doc",
	"affected": {
		"defaultBase": "main"
	},
	"cli": {
		"packageManager": "npm"
	},
	"implicitDependencies": {
		"package.json": {
			"dependencies": "*",
			"devDependencies": "*"
		},
		".eslintrc.json": "*"
	},
	"tasksRunnerOptions": {
		"default": {
			"runner": "nx-cloud",
			"options": {
				"cacheableOperations": ["pre-build", "build", "schematics", "lint", "test", "e2e"],
				"accessToken": "MWFkNWRhOTQtM2ViYi00ZmVlLWI0MjYtN2Q4Y2M2YmRjYmIxfHJlYWQtd3JpdGU="
			}
		}
	},
	"generators": {
		"@nx/angular:application": {
			"style": "scss",
			"linter": "eslint",
			"unitTestRunner": "jest",
			"e2eTestRunner": "cypress"
		},
		"@nx/angular:library": {
			"linter": "eslint",
			"unitTestRunner": "jest"
		},
		"@nx/angular:component": {
			"style": "scss"
		}
	},
	"defaultProject": "ng-doc",
	"targetDefaults": {
		"lint": {
			"inputs": ["default", "{workspaceRoot}/.eslintrc.json"]
		},
		"build": {
			"dependsOn": ["pre-build"]
		},
		"pre-build": {
			"dependsOn": ["^build"]
		}
	}
}
